<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	  <RootNamespace>WebSocketDemo</RootNamespace>
	  <AssemblyName>WebSocketDemo</AssemblyName>
	  <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="icon.png" />
    <None Remove="WebSocketDemo.zip" />
    <None Remove="WebUI\WebSocketTest.html" />
    <None Remove="WebUI\WebSocketTest.js" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="icon.png" />
    <EmbeddedResource Include="WebUI\WebSocketTest.html" />
    <EmbeddedResource Include="WebUI\WebSocketTest.js" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MediaBrowser.Server.Core" Version="4.8.11" />
    <PackageReference Include="System.Memory" Version="4.6.3" />
  </ItemGroup>

</Project>
